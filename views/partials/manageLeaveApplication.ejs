<div id="myleaves" style="margin-top:50px;" class="mainbox col-md-10 col-md-offset-3 col-sm-1 col-sm-offset-2">                    
    <div class="panel panel-info" >
            <div class="panel-heading">
                <div class="panel-title">Manage Leaves</div>
            </div>     

            <div style="padding-top:30px" class="panel-body" > 
                <style>
                   tr, td {
                    padding: 15px;
                    }
                    </style>
                <table >
                    <tr>
                        <td> Leave type</td>
                        <td> From Date</td>
                        <td> To Date</td>
                        <td> Action</td>
                    </th>

                   
                <% if(typeof data!='undefined'){ 
                      for(var i =0;i < data.length;i++){ %>
                    <tr>
                        <td> <%= data[i].type %> </td>
                        <td>  <%= data[i].fromDate %> </td>
                        <td> <%= data[i].toDate %> </td>
                        <td> <a href="/delete_leave/<%=data[i].id%>">Delete</a> </td>
                    </tr>
                    <% } } %>
                </table>
            
            
            </div></div>
            </div>
            


